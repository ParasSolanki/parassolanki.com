---
import Header from "~/components/header.astro";
import BaseLayout from "./base.astro";

interface Props {
  title: string;
  description: string;
  image?: string;
}

const { title, description, image } = Astro.props;
---

<BaseLayout title={title} description={description} image={image}>
  <Header />
  <slot />
</BaseLayout>
